module Widgets.ModelChoiceField exposing (..)

import Browser
import Css
import Html.Styled as Styled exposing (Html, div, h2)
import Html.Styled.Attributes as SA
import Json.Decode exposing (decodeValue)
import Json.Encode exposing (Value)
import List.Extra
import Select exposing (MenuItem, initState, staticSelectIdentifier, update)
import Select.Styles as Styles
import Svg.Styled as SvgStyled
import Svg.Styled.Attributes exposing (..)
import Widgets.Models.ModelChoiceField exposing (Options_(..), Options_Course__, ToModel, toModel)


{-| A djelm widget for the ModelChoiceField Django form field

This widget was generated and tested against the <https://package.elm-lang.org/packages/Confidenceman02/elm-select/10.4.0/> Elm package.

-}
type Msg
    = SelectMsg (Select.Msg Options_)


type alias ReadyModel =
    { selectState : Select.State
    , items : List (MenuItem Options_)
    , selectedItem : Maybe Options_
    , toModel : ToModel
    }


type Model
    = Ready ReadyModel
    | Error


isSelected : Options_ -> Bool
isSelected options =
    case options of
        Course option ->
            option.selected


toIconView : Options_Course__ -> Html Never
toIconView option =
    if String.contains "Django" option.choice_label then
        django_icon

    else if String.contains "Elm" option.choice_label then
        elm_icon

    else if String.contains "Figma" option.choice_label then
        figma_icon

    else if String.contains "photography" option.choice_label then
        shutter_icon

    else if String.contains "Python" option.choice_label then
        python_icon

    else if String.contains "Instagram" option.choice_label then
        insta_icon

    else if String.contains "drawing" option.choice_label then
        pencil_icon

    else if String.contains "Photoshop" option.choice_label then
        ps_icon

    else if String.contains "Neural" option.choice_label then
        neural_icon

    else
        Styled.text ""


toMenuItemView : Options_Course__ -> Html Never
toMenuItemView option =
    let
        inMinutes : Float
        inMinutes =
            toFloat option.instance.duration / 60

        durationHours : Int
        durationHours =
            inMinutes / 60 |> truncate

        durationMinuts : Int
        durationMinuts =
            inMinutes - (60 * toFloat durationHours) |> truncate
    in
    div [ SA.class "option-wrapper" ]
        [ div [ SA.class "icon-wrapper" ] [ toIconView option ]
        , div [ SA.class "course-title" ]
            [ h2 [] [ Styled.text option.choice_label ]
            , Styled.p [] [ Styled.text ("by " ++ option.instance.instructor) ]
            ]
        , div [ SA.class "right-content" ]
            [ div [ SA.class "course-duration" ] [ clock_icon, Styled.text (String.fromInt durationHours ++ "h" ++ " " ++ String.fromInt durationMinuts ++ "min") ]
            , div [ SA.class "course-rating" ] [ flame_icon, Styled.text (String.fromFloat option.instance.rating) ]
            ]
        ]


toMenuItem : Options_ -> MenuItem Options_
toMenuItem options =
    case options of
        (Course option) as opt ->
            Select.customMenuItem { item = opt, label = option.choice_label, view = toMenuItemView option }
                |> Select.valueMenuItem option.value


toReadyModel : ToModel -> ReadyModel
toReadyModel m =
    { selectState = initState (staticSelectIdentifier m.auto_id)
    , items =
        List.map
            toMenuItem
            m.options
    , selectedItem = List.Extra.find isSelected m.options
    , toModel = m
    }


init : Value -> ( Model, Cmd Msg )
init f =
    case decodeValue toModel f |> Result.map toReadyModel of
        Ok m ->
            ( Ready m, Cmd.none )

        Err _ ->
            ( Error, Cmd.none )


main : Program Value Model Msg
main =
    Browser.element
        { init = init
        , view = view >> Styled.toUnstyled
        , update = update
        , subscriptions = \_ -> Sub.none
        }


update : Msg -> Model -> ( Model, Cmd Msg )
update msg model =
    case model of
        Ready readyModel ->
            case msg of
                SelectMsg selectMsg ->
                    let
                        ( maybeAction, selectState, cmds ) =
                            Select.update selectMsg readyModel.selectState

                        updatedSelectedItem =
                            case maybeAction of
                                Just (Select.Select i) ->
                                    Just i

                                Just Select.Clear ->
                                    Nothing

                                _ ->
                                    readyModel.selectedItem
                    in
                    ( Ready
                        { readyModel
                            | selectState = selectState
                            , selectedItem = updatedSelectedItem
                        }
                    , Cmd.map SelectMsg cmds
                    )

        _ ->
            ( model, Cmd.none )


view : Model -> Html Msg
view model =
    case model of
        Ready readyModel ->
            let
                selected =
                    case readyModel.selectedItem of
                        Just opt ->
                            Just (toMenuItem opt)

                        _ ->
                            Nothing

                menuConfig =
                    Styles.setMenuMaxHeightPx (Css.px 390) (Styles.getMenuConfig Styles.bootstrap4)
            in
            Styled.map SelectMsg <|
                Select.view
                    (Select.single selected
                        |> Select.state readyModel.selectState
                        |> Select.menuItems readyModel.items
                        |> Select.placeholder "Select..."
                        |> Select.searchable True
                        |> Select.clearable True
                        |> Select.name readyModel.toModel.name
                        |> Select.setStyles (Styles.setMenuStyles menuConfig Styles.bootstrap4)
                    )

        Error ->
            Styled.text ""



